# Oh My Zsh Configs for Homebrew

# Aliases
alias bbd="brew bundle dump --force"
alias bbi="brew bundle install"
alias bc="brew cleanup"
alias bo="brew outdated"
alias bp="brew pin"
alias bpl="brew list --pinned"
alias bu="brew upgrade"
alias buc="brew upgrade --cask"
alias buf="brew upgrade --formula"
alias bup="brew unpin"

